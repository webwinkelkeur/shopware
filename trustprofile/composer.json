{
    "name": "trustprofile/shopware",
    "description": "TrustProfile integration for Shopware",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "TrustProfile",
            "role": "Manufacturer"
        }
    ],
    "require": {
        "shopware/core": "^6.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "extra": {
        "shopware-plugin-class": "TrustProfile\\Shopware\\TrustProfile",
        "plugin-icon": "Resources/config/plugin.png",
        "label": {
            "en-GB": "TrustProfile",
            "nl-NL": "TrustProfile"
        },
        "description": {
            "en-GB": "Connect your store to TrustProfile.",
            "nl-NL": "Verbind je webshop met TrustProfile."
        }
    },
    "autoload": {
        "psr-4": {
            "Valued\\Shopware\\": "common",
            "TrustProfile\\Shopware\\": "src/"
        }
    }
}
