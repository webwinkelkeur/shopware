#!/bin/bash
cd "$(dirname "$0")/.."

echo "$pwd";
for site in webwinkelkeur trustprofile; do
  set -a
  source $site.env
  set +a
  php templates/config.xml.php > $site/src/Resources/config/config.xml
  php templates/services.xml.php > $site/src/Resources/config/services.xml

  cp -r templates/views $site/src/Resources
  cp -r common $site/src
done
